 * Revision 1.1  2013-11-25 14:21:34  mgostling
 * Added support for TRB1x.
 *
 * Revision 1.1.1.1  2013-09-02 10:22:42  jthiru
 * Adding TRB12 CUTE
 *
 * Revision 1.2  2011/03/22 13:32:49  hchalla
 * Initial Version.
 *
 * Revision 1.1  2011/02/28 11:57:43  hmuneer
 * no message

 */


MEMORY
{
	/* Start code placement at offset 1MB */

	/* ram : o = 0x100000, l = 512k */
	ram : o = 0x100000, l = 1024k
}

SECTIONS
{
	/* Code */

	__text_start = 0x100000;
	.text ABSOLUTE(__text_start) : { *(.text) } > ram
	__text_stop = .;

	/* String Data */

	.rdata : { *(.rdata) } > ram

	/* Initialized Data */

/*	.idata : { *(.idata) } > ram */
	.data : { *(.data) } > ram

	/* BSS */

	__bss_start = .;
	.bss : { *(.bss) } > ram
	__bss_stop = .; 
}
